{
  "name": "sdkroller",
  "version": "1.0.0",
  "description": "a sdk empty cli",
  "main": "src/index.ts",
  "scripts": {
    "build": "rollup -c",
    "lint:js": "cross-env eslint 'src/**/*.{js,ts}'",
    "lint:js-fix": "cross-env eslint 'src/**/*.{js,ts}' --fix",
    "lint:css": "cross-env stylelint 'src/**/*.{css,less}'",
    "lint": "npm run lint:js && npm run lint:css",
    "format": "prettier --write 'src/**/*.{js,ts,css,less}'",
    "postbuild": "node scripts/cleanupDTS.js --debug",
    "eslint:init": "npm init @eslint/config@latest"
  },
  "type": "module",
  "author": "panda-hjn",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.5.0",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@rollup/plugin-url": "^8.0.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.4.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.4.0",
    "less": "^4.2.0",
    "postcss": "^8.4.38",
    "prettier": "3.3.2",
    "rollup": "^4.18.0",
    "rollup-plugin-postcss": "^4.0.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "8.0.0-alpha.30"
  }
}
